<div class="container-fluid container-fluid-custom pb-5 mb-5">
  <div class="row ro-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if(searchResult != '') { %> <% searchResult.forEach(function (note,
    index) { %>
    <a href="/dashboard/item/<%= note._id %>">
      <div class="col">
        <div class="p-3 border bg-light">
          <h5 class="text-center fw-bold text-primary"><%= note.title %></h5>
        </div>
      </div>
    </a>

    <% }) %> <% } else { %>
    <div class="row mt-4">
      <div class="col">
        <h3 class="text-center">No notes found...</h3>
        <p class="text-center">Try searching for something else!</p>
      </div>
      <% } %>
    </div>
  </div>
</div>
